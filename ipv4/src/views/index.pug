    .container
      .background.lazyload(data-src='img/pricing-bg.png')
      .row
        //block
        .block-list.col-md-4
          .block-list__container
            .block-list__content
              h2.block-list__title Аренда <span>IPv4</span> адреса
              ul.block-list__list
                li.block-list__item
                  span.block-list__item-icon IP
                  span.block-list__item-title 256IP Адресов
                  span.block-list__item-price 7 000 ₽ 
                li.block-list__item
                  span.block-list__item-icon IP
                  span.block-list__item-title 512IP Адресов
                  span.block-list__item-price 14 000 ₽
                li.block-list__item
                  span.block-list__item-icon IP
                  span.block-list__item-title 1024IP Адресов
                  span.block-list__item-price 28 000 ₽
            a(href="#").button.block-list__button Подробнее 
              svg.icon
                use(xlink:href='img/icons/icons-sprite.svg#arrow-right')
        //block
        .block-list.col-md-4
          .block-list__container
            .block-list__content
              h2.block-list__title Покупка <span>IPv4</span> адреса
              ul.block-list__list
                li.block-list__item
                  span.block-list__item-icon AS
                  span.block-list__item-title Автономная система
                  span.block-list__item-price 7 800 ₽ / разово
                li.block-list__item
                  span.block-list__item-icon IP
                  span.block-list__item-title 256IP Адресов
                  span.block-list__item-price 47 000 ₽ / год
                li.block-list__item
                  span.block-list__item-icon IP
                  span.block-list__item-title 512IP Адресов
                  span.block-list__item-price По запросу
                li.block-list__item
                  span.block-list__item-icon IP
                  span.block-list__item-title 1024IP Адресов
                  span.block-list__item-price По запросу
                li.block-list__item
                  span.block-list__item-icon IP
                  span.block-list__item-title 2048IP Адресов
                  span.block-list__item-price По запросу
                li.block-list__item
                  span.block-list__item-icon IP
                  span.block-list__item-title 4096IP Адресов
                  span.block-list__item-price По запросу
            a(href="#").button.block-list__button Подробнее 
              svg.icon
                use(xlink:href='img/icons/icons-sprite.svg#arrow-right')
        //block
        .block-list.col-md-4
          .block-list__container
            .block-list__content
              h2.block-list__title Получение <span>IPv4</span> адреса через <span>LIR</span>
              ul.block-list__list
                li.block-list__item
                  span.block-list__item-icon.block-list__item-icon_smaller LIR
                  span.block-list__item-title Консультационные услуги по регистрации LIR
                  span.block-list__item-price 23 600 ₽
                li.block-list__item
                  span.block-list__item-icon.block-list__item-icon_smaller RIPE
                  span.block-list__item-title Ежегодный платеж в RIPE NCC 
                  span.block-list__item-price 110 000 ₽
                li.block-list__item
                  span.block-list__item-icon.block-list__item-icon_smaller RIPE
                  span.block-list__item-title Регистрационный платеж в RIPE NCC
                  span.block-list__item-price 158 000 ₽
            a(href="#").button.block-list__button Подробнее 
              svg.icon
                use(xlink:href='img/icons/icons-sprite.svg#arrow-right')
