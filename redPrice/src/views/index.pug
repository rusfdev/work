  //chat
  .chat-block
    a(href="javascript:void(0);" data-chat-close).chat-block__close
      svg(viewBox='0 0 32 32', stroke-width='2')
        circle(cx='16', cy='16', r='15')
        path(d='M10.2568 10.3431L21.5705 21.6568', stroke-linecap='round')
        path(d='M10.2568 21.6569L21.5705 10.3432', stroke-linecap='round')

    .chat-block__head
      h2.chat-block__title Напишите ваше сообщение
      span.chat-block__label Операторы онлайн
      img.chat-block__head-logo.lazy(alt='Логотип', src='/static/assets/images/svg/logotype/footlogo.svg')
    .chat-block__content  
      .chat-block__messages
        .chat-block__messages-content
          .chat-block__message.chat-block__message_user 
            .chat-block__message-content Добрый день
          .chat-block__message.chat-block__message_admin
            .chat-block__message-name Елена
            .chat-block__message-container
              .chat-block__message-image
                img(src="data:image/png;base64,", alt="image")
              .chat-block__message-content Здравствуйте! Чем могу помочь?
          .chat-block__message.chat-block__message_user 
            .chat-block__message-content У меня есть большая проблема
          .chat-block__message.chat-block__message_admin
            .chat-block__message-name Елена
            .chat-block__message-container
              .chat-block__message-image
                img(src="data:image/png;base64,", alt="image")
              .chat-block__message-content Ничем не могу помочь.

      form(action="#").chat-block__send-form
        input(type="text" placeholder="Введите сообщение").chat-block__send-input
        button(type='submit').chat-block__send-button 
          svg(viewBox='0 0 16 16')
            path(d='M15.6691 7.48064L0.81228 0.623664C0.575714 0.516238 0.293435 0.580237 0.128868 0.78366C-0.0368421 0.987084 -0.0425563 1.27622 0.115154 1.48536L5.00075 7.99948L0.115154 14.5136C-0.0425563 14.7227 -0.0368421 15.013 0.127725 15.2153C0.23858 15.3536 0.40429 15.4279 0.572286 15.4279C0.653427 15.4279 0.734568 15.4107 0.811137 15.3753L15.6679 8.51833C15.8713 8.42462 16.0005 8.22234 16.0005 7.99948C16.0005 7.77663 15.8713 7.57435 15.6691 7.48064Z')
      form(action="#").chat-block__info-form
        .chat-block__info-form-item
          input#id_name.form-control.chat-block__info-form-input(maxlength='255', name='name', placeholder='Как Вас зовут', type='text')
        .chat-block__info-form-item
          input#id_phone.form-control.chat-block__info-form-input(maxlength='50', name='phone', placeholder='Ваш телефон', type='text')
        .chat-block__info-form-item
          label.custom_checkbox.custom_checkbox__checked
            span
              svg(fill='none', height='11', viewBox='0 0 11 11', width='11', xmlns='http://www.w3.org/2000/svg', style='display: inline;')
                path(d='M0 9.8995L5.65685 10.1067L3.53553 0', stroke='#C1C1C1', transform='translate(7.5 -1.5) rotate(45)')
            input#id_agree(name='agree', type='checkbox', checked='checked')
            | Согласие на обработку персональных данных.
        .chat-block__info-form-item
          button(type='submit').btn.btn-primary.btn-xl.chat-block__info-form-button Отправить


  //popup
  .popup#succes.popup-message
    .popup-block
      .popup-block__container
        .popup-message__icon
          svg(viewBox='0 0 36 37')
            path(d='M35.07 3.40213L12.3725 36.9079L0.232422 24.7678L3.76796 21.2322L11.6278 29.0921L30.9304 0.597885L35.07 3.40213Z')
        h5.popup-message__title Сообщение отправлено
        p.popup-message__text Мы свяжемся с вами в ближайшее время
        a(href='javascript:void(0);' data-popup-close).btn.btn-primary.btn-xl.popup-message__button Хорошо
